{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
  "parameters": {
    "networkSettings": {
      "type": "object",
      "metadata": {
        "description": "Object array with all information to create Azure Foundation networking information."
      }
    }
  },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/ipGroups",
            "apiVersion": "2020-05-01",
            "name": "[parameters('networkSettings').ipGroups[copyIndex()].name]",
            "location": "[parameters('networkSettings').ipGroups[copyIndex()].location]",
            "copy": {
                "name": "IPGroupsLoop",
                "count": "[length(parameters('networkSettings').ipGroups)]"
            },
            "properties": {
                "ipAddresses": [
                    "[parameters('networkSettings').ipGroups[copyIndex()].ipAddresses]"
                ]
            }
        }
    ]
}