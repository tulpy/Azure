{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "VendorName",
    "AlertType",
    "StartTimeUtc",
    "TimeGenerated",
    "ReportedSeverity",
    "SystemAlertId",
    "AzureResourceId",
    "AzureResourceSubscriptionId",
    "WorkspaceId",
    "WorkspaceSubscriptionId",
    "WorkspaceResourceGroup",
    "AgentId",
    "CompromisedEntity",
    "Intent",
    "AlertDisplayName",
    "Description",
    "RemediationSteps",
    "ExtendedProperties",
    "ResourceIdentifiers"
  ],
  "properties": {
    "VendorName": {
      "$id": "#/properties/VendorName",
      "type": "string",
      "title": "The Vendorname Schema",
      "default": "",
      "examples": [
        "Microsoft"
      ],
      "pattern": "^(.*)$"
    },
    "AlertType": {
      "$id": "#/properties/AlertType",
      "type": "string",
      "title": "The Alerttype Schema",
      "default": "",
      "examples": [
        "SUSPECT_SVCHOST"
      ],
      "pattern": "^(.*)$"
    },
    "StartTimeUtc": {
      "$id": "#/properties/StartTimeUtc",
      "type": "string",
      "title": "The Starttimeutc Schema",
      "default": "",
      "examples": [
        "2016-12-20T13:38:00.000Z"
      ],
      "pattern": "^(.*)$"
    },
    "EndTimeUtc": {
      "$id": "#/properties/EndTimeUtc",
      "type": "string",
      "title": "The Endtimeutc Schema",
      "default": "",
      "examples": [
        "2019-12-20T13:40:01.733Z"
      ],
      "pattern": "^(.*)$"
    },
    "ProcessingEndTime": {
      "$id": "#/properties/ProcessingEndTime",
      "type": "string",
      "title": "The Processingendtime Schema",
      "default": "",
      "examples": [
        "2019-09-16T12:10:19.5673533Z"
      ],
      "pattern": "^(.*)$"
    },
    "TimeGenerated": {
      "$id": "#/properties/TimeGenerated",
      "type": "string",
      "title": "The Timegenerated Schema",
      "default": "",
      "examples": [
        "2016-12-20T13:38:03.000Z"
      ],
      "pattern": "^(.*)$"
    },
    "IsIncident": {
      "$id": "#/properties/IsIncident",
      "type": "boolean",
      "title": "The Isincident Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "Severity": {
      "$id": "#/properties/Severity",
      "type": "string",
      "title": "The Severity Schema",
      "default": "",
      "examples": [
        "High"
      ],
      "pattern": "^(.*)$"
    },
    "Status": {
      "$id": "#/properties/Status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "New"
      ],
      "pattern": "^(.*)$"
    },
    "ProductName": {
      "$id": "#/properties/ProductName",
      "type": "string",
      "title": "The Productname Schema",
      "default": "",
      "examples": [
        "Azure Security Center"
      ],
      "pattern": "^(.*)$"
    },
    "SystemAlertId": {
      "$id": "#/properties/SystemAlertId",
      "type": "string",
      "title": "The Systemalertid Schema",
      "default": "",
      "examples": [
        "2342409243234234_F2BFED55-5997-4FEA-95BD-BB7C6DDCD061"
      ],
      "pattern": "^(.*)$"
    },
    "AzureResourceId": {
      "$id": "#/properties/AzureResourceId",
      "type": "string",
      "title": "The Azureresourceid Schema",
      "default": "",
      "examples": [
        "/subscriptions/86057C9F-3CDD-484E-83B1-7BF1C17A9FF8/resourceGroups/backend-srv/providers/Microsoft.Compute/WebSrv1"
      ],
      "pattern": "^(.*)$"
    },
    "AzureResourceSubscriptionId": {
      "$id": "#/properties/AzureResourceSubscriptionId",
      "type": "string",
      "title": "The Azureresourcesubscriptionid Schema",
      "default": "",
      "examples": [
        "86057C9F-3CDD-484E-83B1-7BF1C17A9FF8"
      ],
      "pattern": "^(.*)$"
    },
    "WorkspaceId": {
      "$id": "#/properties/WorkspaceId",
      "type": "string",
      "title": "The Workspaceid Schema",
      "default": "",
      "examples": [
        "077BA6B7-8759-4F41-9F97-017EB7D3E0A8"
      ],
      "pattern": "^(.*)$"
    },
    "WorkspaceSubscriptionId": {
      "$id": "#/properties/WorkspaceSubscriptionId",
      "type": "string",
      "title": "The Workspacesubscriptionid Schema",
      "default": "",
      "examples": [
        "86057C9F-3CDD-484E-83B1-7BF1C17A9FF8"
      ],
      "pattern": "^(.*)$"
    },
    "WorkspaceResourceGroup": {
      "$id": "#/properties/WorkspaceResourceGroup",
      "type": "string",
      "title": "The Workspaceresourcegroup Schema",
      "default": "",
      "examples": [
        "omsrg"
      ],
      "pattern": "^(.*)$"
    },
    "AgentId": {
      "$id": "#/properties/AgentId",
      "type": "string",
      "title": "The Agentid Schema",
      "default": "",
      "examples": [
        "5A651129-98E6-4E6C-B2CE-AB89BD815616"
      ],
      "pattern": "^(.*)$"
    },
    "CompromisedEntity": {
      "$id": "#/properties/CompromisedEntity",
      "type": "string",
      "title": "The Compromisedentity Schema",
      "default": "",
      "examples": [
        "WebSrv1"
      ],
      "pattern": "^(.*)$"
    },
    "Intent": {
      "$id": "#/properties/Intent",
      "type": "string",
      "title": "The Intent Schema",
      "default": "",
      "examples": [
        "Execution"
      ],
      "pattern": "^(.*)$"
    },
    "AlertDisplayName": {
      "$id": "#/properties/AlertDisplayName",
      "type": "string",
      "title": "The Alertdisplayname Schema",
      "default": "",
      "examples": [
        "Suspicious process detected"
      ],
      "pattern": "^(.*)$"
    },
    "Description": {
      "$id": "#/properties/Description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "Suspicious process named ‘SVCHOST.EXE’ was running from path: %{Process Path}"
      ],
      "pattern": "^(.*)$"
    },
    "RemediationSteps": {
      "$id": "#/properties/RemediationSteps",
      "type": "array",
      "title": "The Remediationsteps Schema",
      "items": {
        "$id": "#/properties/RemediationSteps/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "contact your security information team"
        ],
        "pattern": "^(.*)$"
      }
    },
    "ExtendedProperties": {
      "$id": "#/properties/ExtendedProperties",
      "type": "object",
      "title": "The Extendedproperties Schema",
      "required": [
        "Process Path",
        "Account",
        "PID",
        "ActionTaken"
      ],
      "properties": {
        "Process Path": {
          "$id": "#/properties/ExtendedProperties/properties/Process Path",
          "type": "string",
          "title": "The Process path Schema",
          "default": "",
          "examples": [
            "c:\\temp\\svchost.exe"
          ],
          "pattern": "^(.*)$"
        },
        "Account": {
          "$id": "#/properties/ExtendedProperties/properties/Account",
          "type": "string",
          "title": "The Account Schema",
          "default": "",
          "examples": [
            "Contoso\\administrator"
          ],
          "pattern": "^(.*)$"
        },
        "PID": {
          "$id": "#/properties/ExtendedProperties/properties/PID",
          "type": "integer",
          "title": "The Pid Schema",
          "default": 0,
          "examples": [
            944
          ]
        },
        "ActionTaken": {
          "$id": "#/properties/ExtendedProperties/properties/ActionTaken",
          "type": "string",
          "title": "The Actiontaken Schema",
          "default": "",
          "examples": [
            "Detected"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "Entities": {
      "$id": "#/properties/Entities",
      "type": "array",
      "title": "The Entities Schema"
    },
    "ResourceIdentifiers": {
      "$id": "#/properties/ResourceIdentifiers",
      "type": "array",
      "title": "The Resourceidentifiers Schema"
    }
  }
}

